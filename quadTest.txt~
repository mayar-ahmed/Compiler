program	
int i;
int j;
int n;
int f1;
int f2;
bool found;


begin

n=100;
found=false;
while(found!=true)
{
	for(i=1:i<100:1)
	{
		for(j=i+1:j<100:1)
		{
			if(i*j==n)then
				found=true;
				break;
			else
				if(i*j<n)then
					continue;
				endif;
				if(i*j>n)then
					break;
				endif;
			endif;
		};
		if(found==true)then
			break;		
		endif;
	};
	f1=i;
	f2=j;
};

n=f1*f2;
end
