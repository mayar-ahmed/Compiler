program	
int i;
int j;
int n;
int f1;
int f2;
bool found;


begin

n=100;
found=false;
f1=0;
f2=33;
for(j=f1-f2:j<f1*f2:-5)
{
	do
	{
		if(n>44 and (f2==f1 or found == true))then
			found=true;
			break;
		else
			found = not found;
			continue;
		endif;
		f1=2;
	}while(found==true);
	if(not found)then
		break;	
	endif;
	n=n+1;
};

switch(f1)
{
	case -3:
		for(i=3:i<=33:3)
		{
			if(i<10)then
				continue;
			else
				found=false;
				break;
			endif;	
			f1=4;	
		};
		f1=3;
		break;
	case 0:
		found=true;
		break;
	default:
		i=i+8;

};


i=n*n;
end
